{
  "extends": [
    "tslint-config-standard"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-ordering": [
      true,
      {
        "order": "statics-first"
      }
    ],
    "no-angle-bracket-type-assertion": false,
    "no-floating-promises": false,
    "no-unused-variable": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "asyncArrow": "always",
        "named": "never"
      }
    ],
    "trailing-comma": [
      true,
      {
        "esSpecCompliant": true,
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-type-operator",
      "check-preblock"
    ]
  }
}
